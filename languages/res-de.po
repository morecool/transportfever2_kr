msgid "VEHICLE_TRUCK_FAW_JIEFANG_J6P_TANKER_NAME"
msgstr "FAW Jiefang J6P Tank-Lkw"

msgid "VEHICLE_TRUCK_FAW_JIEFANG_J6P_TIPPER_DESCRIPTION"
