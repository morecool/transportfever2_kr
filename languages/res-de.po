msgid "VEHICLE_TRUCK_FAW_JIEFANG_J6P_UNIVERSAL_NAME"
msgstr "FAW Jiefang J6P Planen-Lkw"

msgid "VEHICLE_TRUCK_FORD_M77_DESCRIPTION"
msgstr ""
"Ein allgegenwärtiger Lkw, gebaut als Standardplattform mit vielen "
"verschiedenen Aufbauten."

msgid "VEHICLE_TRUCK_FORD_M77_NAME"
msgstr "Ford Model 77 Rungen-Lkw"

