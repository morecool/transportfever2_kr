msgstr "建設地1"

msgid "MISSION_TRANSSIB_PARAMS_CONSTRUCTION_SITE2"
msgstr "建設地2"

msgid "MISSION_TRANSSIB_PARAMS_CONSTRUCTION_SITE3"
